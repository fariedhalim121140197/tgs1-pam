+package com.example.myapplication
+import android.os.Bundle
+import androidx.appcompat.app.AppCompatActivity
+import kotlinx.android.synthetic.main.activity_display_data.*
+
+class DisplayDataActivity : AppCompatActivity() {
+
+    override fun onCreate(savedInstanceState: Bundle?) {
+        super.onCreate(savedInstanceState)
+        setContentView(R.layout.activity_display_data)
+
+        val name = intent.getStringExtra("NAME")
+        val gender = intent.getStringExtra("GENDER")
+        val agreeToTerms = intent.getBooleanExtra("AGREE_TO_TERMS", false)
+        val country = intent.getStringExtra("COUNTRY")
+
+        textViewName.text = "Name: $name"
+        textViewGender.text = "Gender: $gender"
+        textViewTerms.text = "Agree to Terms: ${if (agreeToTerms) "Yes" else "No"}"
+        textViewCountry.text = "Country: $country"
+    }
+}
